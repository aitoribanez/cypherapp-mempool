{
  "name": "cypherapp-mempool",
  "version": "0.1.1",
  "description": "Get Bitcoin node's mempool information using cyphernode.",
  "scripts": {
    "server": "cd server;npm run start",
    "server-deps": "cd server; npm i",
    "client-dev": "cd client; npm run serve",
    "client-build": "cd client; npm run build",
    "client-deps": "cd client; npm i",
    "docker-build-image": "docker build -t cypherapp-mempool:v0.1.1 .",
    "docker-run": "docker run --rm -it -d -p 3000:3000 --network cyphernodenet cypherapp-mempool:v0.1.1",
    "docker-compose-up": "docker-compose up -d"
  },
  "keywords": ["cyphernode", "cypherapp", "bitcoin"],
  "author": "Aitor Iba√±ez <aitiba@gmail.com>",
